#include <bits/stdc++.h>
using namespace std;

int main() {
	int time2,range;
	cin>>time2;
	while(time2--){
	    cin>>range;
	    int arr[range];
	    for(int i=0;i<range;i++){
	        cin>>arr[i];
	    }
	    int count=0;
	    for(int i=0;i<range;i++){
	        if(arr[i]>=1000)
	          count++;
	    }
	    cout<<count<<endl;
	}
   return 0;
}
